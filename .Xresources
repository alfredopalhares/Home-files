URxvt.geometry: 400x30
URxvt.buffered:	true
URxvt*termName: screen-256color
URxvt.background:	black
URxvt.foreground:	white
URxvt.cursorColor: yellow
URxvt.underlineColor:	green
! Depends of the ttf-inconsolata package
URxvt.font:xft:inconsolata:pixelsize=16:	antialias=false
! Enable Shift-PageUp/Down in screen
URxvt.secondaryScroll: true
! Disable the srollbar
URxvt.scrollBar: false
! Load the extensions
! macther is for weblink click support
! url-select is to enable yankable urls
URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select
!urxvt-perls options
!Url-select
URvxt.url-select.button: 1
URxvt.url-select.launcher: /usr/bin/luakit
URxvt.url-select.underline: true
URxvt.keysym.M-u: perl:url-select:select_next
! Keyboard select stuff
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
